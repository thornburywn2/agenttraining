{
  "name": "agents-md-demo",
  "version": "1.0.0",
  "description": "A modern full-stack TypeScript application showcasing AGENTS.md framework integration with CLAUDE.md development standards",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5175",
    "dev:backend": "PORT=5176 tsx watch server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "tsc && vite build",
    "build:backend": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:a11y": "eslint . --ext tsx --plugin jsx-a11y",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\" \"server/**/*.{ts,md}\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "security:audit": "npm audit",
    "validate:env": "tsx scripts/validate-env.ts"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/marked": "^5.0.2",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "fastify": "^4.25.2",
    "highlight.js": "^11.11.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.309.0",
    "marked": "^16.4.0",
    "puppeteer": "^24.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.1.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1"
  },
  "keywords": [
    "agents.md",
    "claude.md",
    "typescript",
    "react",
    "fastify",
    "accessibility",
    "wcag",
    "ai-agents"
  ],
  "author": "",
  "license": "MIT"
}
